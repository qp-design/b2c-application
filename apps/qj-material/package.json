{
  "name": "qj-material",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "rm -rf build/dist/ && rm -rf dist/",
    "build": "webpack --mode production --config ./scripts/build.js",
    "postbuild": "cp -R ./dist ./build",
    "build:dev": "webpack --mode development --config ./scripts/start.js",
    "local": "prettier --write src/",
    "start": "webpack serve --mode development --config ./scripts/start.js --live-reload --hot --no-client-overlay",
    "start:live": "webpack serve --mode development --config ./scripts/start.js --live-reload --hot --no-client-overlay"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.5",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "html-webpack-plugin": "^5.3.2",
    "json-loader": "^0.5.7",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.0",
    "tsconfig": "*",
    "typescript": "^4.5.2",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@brushes/qj-shared-library": "^0.1.2",
    "@brushes/share-resource": "^1.1.2",
    "@brushes/shared-store": "^0.0.3",
    "antd": "^5.8.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "s-material-react": "*"
  }
}
