{
  "name": "qj-operate",
  "version": "1.0.0",
  "scripts": {
    "first": "rm -rf build/ && rm -rf dist/ && prettier --write src/",
    "second": "webpack --mode production --config scripts/build.js",
    "prebuild:pro": "npm run first",
    "prebuild": "npm run first",
    "build:pro": "npm run second",
