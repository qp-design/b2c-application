{
  "name": "s-material-react",
  "version": "1.5.7",
  "description": "'组件库'",
  "main": "dist/index.js",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "files": [
    "dist",
    "css",
    "iconfont"
  ],
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:ts": "rollup -c -w",
    "start:css": "npx nodemon --config ./nodemon.json",
    "cp-css": "rm -rf css/ && mkdir -p ./css && cp -R src/scss/* ./css",
    "build": "npm run cp-css && rm -rf dist/ && prettier --write src/ && rollup -c",
    "push": "git add . && git commit -m",
    "postpush": "git push",
    "prepublishOnly": "echo 我们准备发布npm包",
    "publishOnly": "npm run build && npm run push",
    "postpublishOnly": "npm version patch && npm publish && git push"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@brushes/simulate-component": "^0.2.8",
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "@brushes/mobile-form": "*",
    "@brushes/request": "^0.2.1",
    "@brushes/shared-utils": "*",
    "@brushes/utils": "latest",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node": ">=18.15.7",
    "@types/react": "^18.0.28",
    "concurrently": "^7.6.0",
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "qj-b2c-api": "*",
    "qj-mobile-store": "*",
    "react": "^18.2.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "tsconfig": "*",
    "typescript": ">=5.0.2"
  }
}
